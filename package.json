{
  "name": "weedar",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:prod": "ENVFILE=.env.production react-native run-android",
    "android:stage": "ENVFILE=.env.staging react-native run-android",
    "cc": "rm -rf node_modules/.cache/babel-loader/*",
    "android:apk-dev": "ENVFILE=.env.development && react-native run-android --variant=release",
    "android:apk-prod": "ENVFILE=.env.production react-native run-android --variant=release",
    "android:apk-stage": "ENVFILE=.env.staging react-native run-android --variant=release",
    "android:cc": "cd android && ./gradlew clean",
    "android:aab-dev": "cd android && ENVFILE=.env.development ./gradlew bundleRelease",
    "android:aab-stage": "cd android && ENVFILE=.env.staging ./gradlew bundleRelease",
    "android:aab-prod": "cd android && ENVFILE=.env.production ./gradlew bundleRelease",
    "android:code-push-prod": "appcenter codepush release-react -a Weedar/weedar-android -d Production",
    "android:code-push-dev": "appcenter codepush release-react -a Weedar/weedar-android -d Dev",
    "android:code-push-stage": "appcenter codepush release-react -a Weedar/weedar-android -d Staging",
    "ios:code-push-prod": "appcenter codepush release-react -a Weedar/weedar-ios -d Production",
    "ios:code-push-dev": "appcenter codepush release-react -a Weedar/weedar-ios -d Dev",
    "ios:code-push-stage": "appcenter codepush release-react -a Weedar/weedar-ios -d Staging",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "check-types": "tsc --skipLibCheck",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint": "yarn run eslint --fix && yarn run check-types",
    "prepare": "husky install",
    "android:firebase-prod": "cd android && fastlane firebase_upload env:prod",
    "android:firebase-stage": "cd android && fastlane firebase_upload env:stage",
    "android:firebase-dev": "cd android && fastlane firebase_upload env:dev",
    "ios:firebase-prod": "cd ios && fastlane firebase_upload env:prod",
    "ios:firebase-stage": "cd ios && fastlane firebase_upload env:stage",
    "ios:firebase-dev": "cd ios && fastlane firebase_upload env:dev",
    "ios:testflight-prod": "cd ios && fastlane testflight_upload env:prod",
    "ios:testflight-stage": "cd ios && fastlane testflight_upload env:stage",
    "ios:testflight-dev": "cd ios && fastlane testflight_upload env:dev"
  },
